    "editor.selectionBackground": "#264f78",
    "editor.selectionHighlightBackground": "#1f2937",
    "editor.findMatchBackground": "#ffd33d44",
    "editor.findMatchHighlightBackground": "#ffd33d22",
    "editorWhitespace.foreground": "#484f58",
    "editorIndentGuide.background": "#21262d",
    "editorIndentGuide.activeBackground": "#58a6ff",
    "editorLineNumber.foreground": "#6e7681",
    "editorLineNumber.activeForeground": "#f0f6fc",
    "sideBar.background": "#010409",
    "sideBar.foreground": "#e6edf3",
    "sideBar.border": "#21262d",
    "sideBarTitle.foreground": "#f0f6fc",
    "sideBarSectionHeader.background": "#161b22",
    "sideBarSectionHeader.foreground": "#e6edf3",
    "terminal.background": "#0d1117",
    "terminal.foreground": "#e6edf3",
    "terminal.ansiBlack": "#484f58",
    "terminal.ansiRed": "#ff7b72",
    "terminal.ansiGreen": "#3fb950",
    "terminal.ansiYellow": "#d29922",
    "terminal.ansiBlue": "#58a6ff",
    "terminal.ansiMagenta": "#bc8cff",
    "terminal.ansiCyan": "#39c5cf",
    "terminal.ansiWhite": "#b1bac4",
    "list.hoverBackground": "#161b2244",
    "list.hoverForeground": "#e6edf3",
    "list.focusBackground": "#21262d",
    "list.focusForeground": "#e6edf3",
    "list.activeSelectionBackground": "#21262d",
    "list.activeSelectionForeground": "#e6edf3",
    "list.inactiveSelectionBackground": "#161b22",
    "dropdown.background": "#161b22",
    "dropdown.listBackground": "#161b22",
    "dropdown.border": "#30363d",
    "dropdown.foreground": "#e6edf3",
    "activityBar.background": "#010409",
    "activityBar.foreground": "#e6edf3",
    "activityBar.activeBorder": "#fd7e14",
    "activityBar.activeBackground": "#fd7e1422",
    "activityBar.inactiveForeground": "#6e7681",
    "activityBarBadge.background": "#fd7e14",
    "activityBarBadge.foreground": "#0d1117",
    "tab.activeBackground": "#0d1117",
    "tab.activeBorder": "#fd7e14",
    "tab.activeForeground": "#f0f6fc",
    "tab.inactiveBackground": "#010409",
    "tab.inactiveForeground": "#8b949e",
    "tab.border": "#21262d",
    "editorGroupHeader.tabsBackground": "#010409",
    "editorGroupHeader.tabsBorder": "#21262d",
    "notifications.foreground": "#e6edf3",
    "notifications.background": "#161b22",
    "notifications.border": "#30363d",
    "notificationCenterHeader.foreground": "#8b949e",
    "notificationCenterHeader.background": "#161b22",
    "input.background": "#0d1117",
    "input.foreground": "#e6edf3",
    "input.border": "#30363d",
    "input.placeholderForeground": "#7d8590",
    "inputOption.activeBorder": "#fd7e14",
    "inputValidation.errorBackground": "#0e1116",
    "inputValidation.errorBorder": "#f85149",
    "statusBar.background": "#0d1117",
    "statusBar.foreground": "#e6edf3",
    "statusBar.border": "#21262d",
    "statusBar.debuggingBackground": "#fd7e14",
    "statusBar.debuggingForeground": "#0d1117",
    "statusBarItem.hoverBackground": "#161b2244",
    "statusBarItem.remoteBackground": "#58a6ff",
    "statusBarItem.remoteForeground": "#0d1117",
    "editorBracketMatch.background": "#3fb95044",
    "editorBracketMatch.border": "#3fb950",
    "peekViewEditor.background": "#0d1117",
    "peekViewResult.background": "#161b22",
    "peekViewTitle.background": "#161b22",
    "peekViewTitleLabel.foreground": "#e6edf3",
    "peekViewTitleDescription.foreground": "#8b949e",
    "button.background": "#238636",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#2ea043",
    "button.secondaryBackground": "#21262d",
    "button.secondaryForeground": "#f0f6fc",
    "button.secondaryHoverBackground": "#30363d",
    "progressBar.background": "#fd7e14",
    "editorError.foreground": "#f85149",
    "editorWarning.foreground": "#d29922",
    "editorInfo.foreground": "#58a6ff",
    "editorHint.foreground": "#3fb950",
    "panel.background": "#0d1117",
    "panel.border": "#21262d",
    "panelTitle.activeBorder": "#fd7e14",
    "panelTitle.activeForeground": "#f0f6fc",
    "panelTitle.inactiveForeground": "#8b949e",
    "titleBar.activeBackground": "#0d1117",
    "titleBar.activeForeground": "#e6edf3",
    "titleBar.inactiveBackground": "#010409",
    "titleBar.inactiveForeground": "#8b949e",
    "titleBar.border": "#21262d",
    "menubar.selectionForeground": "#e6edf3",
    "menubar.selectionBackground": "#161b22",
    "menu.foreground": "#e6edf3",
    "menu.background": "#161b22",
    "menu.selectionForeground": "#e6edf3",
    "menu.selectionBackground": "#21262d",
    "scrollbar.shadow": "#0008",
    "scrollbarSlider.background": "#6e768166",
    "scrollbarSlider.hoverBackground": "#6e768188",
    "scrollbarSlider.activeBackground": "#6e7681bb",
    "settings.headerForeground": "#e6edf3",
    "settings.modifiedItemIndicator": "#fd7e14",
    "welcomePage.buttonBackground": "#21262d",
    "welcomePage.buttonHoverBackground": "#30363d",
    "debugToolBar.background": "#161b22",
    "debugToolBar.border": "#21262d"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8b949e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation & Operators",
      "scope": [
        "punctuation",
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#ff7b72"
      }
    },
    {
      "name": "Keywords & Control Flow",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.import",
        "keyword.control.export",
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ff7b72",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types & Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.interface",
        "entity.name.enum",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#ffa657",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#d2a8ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.property",
        "variable.other.object.property",
        "support.variable"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Constants & Numbers",
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "name": "String Punctuation",
      "scope": [
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#79c0ff"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "entity.name.section.markdown"],
      "settings": {
        "foreground": "#79c0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#a5d6ff"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#58a6ff"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ffa657",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ffa657",
        "fontStyle": "italic"
      }
    }
  ]
}
